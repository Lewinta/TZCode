 "html": "<div class=\"content\">\r\n    <div>\r\n        <div class=\"print-heading\">\r\n            <div style=\"height:100px; float:left; width:100%;\">\r\n                <img src=\"/files/Logo%20DMD.jpg\" style=\"width: 16% !important; margin-left: 5px; margin-right: 3px; margin-top: 6px; float:left\">\r\n                <table> \r\n                    <tr>\r\n                        <td colspan=4>\r\n                            <img src=\"/files/DMD.jpeg\" style=\"width: 70% !important; margin-top: 5px;\">\r\n                        </td>\r\n                    </tr>\r\n                    <tr>\r\n                        <td style=\"color: #2a51e4; font-size:10px; width: 189px;border-left:1px solid #2a51e4; margin-right: 20px;\">\r\n                                CALLE OSVALDO BAEZ NO.3\r\n                                GAZCUE, SANTO DOMINGO D.N.\r\n                                REPUBLICA DOMINICANA, 10204\r\n                                RNC 101791071\r\n                        </td>\r\n                        <td style=\"color: #2a51e4; font-size:10px;border-left:1px solid #2a51e4;\">\r\n                                (809) 687-0001<br>\r\n                                dmd@dmdrd.com<br>\r\n                                <u>www.dmdrd.com</u>\r\n                        </td>\r\n                        <td class=\"ncf\" style=\"font-size:10px; text-align:center\" >\r\n                               NCF: {{doc.ncf}}<br>\r\n                               FACTURA DE CREDITO FISCAL<br>\r\n                               VALIDA HASTA 31/12/2023\r\n                        </td>\r\n                    </tr>\r\n                </table>\r\n            </div>\r\n        </div>\r\n        \r\n        {% set moneda = (doc.in_words).split() %}\r\n        <div class=\"row_section-break\" style=\"width:100% !important; marin-top: 20px;\">\r\n            <div class=\"col-xs-4 column-break\">\r\n                <div>\r\n                    <table class=table1-a style=\"width:125% !important; margin-right: 0% !important;\">\r\n                        <tbody>\r\n                            <tr>\r\n                                <th style=\"font-size:15px\">DATOS DE CLIENTE</th>\r\n                            </tr>\r\n                            <tr>\r\n                                <td>\r\n                                    \r\n                                    <div style=\"width: auto; height: 140px; \">\r\n                                        <p style=\"font-size:10px\"><b>FECHA: </b> {{doc.posting_date}}</p>\r\n              \r\n                                        {% set razon_social = doc.customer %}\r\n                                        {% set nombre_comercial = frappe.db.get_value(\"Customer\",doc.customer,\"brand_name\") %}\r\n                                        \r\n                                        {% if nombre_comercial %}\r\n                                            {% if razon_social == nombre_comercial %}\r\n                                                <p class=font><b>NOMBRE O RAZON SOCIAL: </b>{{razon_social}}</p>\r\n                                            {% else %}\r\n                                                <p class=font><b>NOMBRE O RAZON SOCIAL: </b>{{razon_social}}</p>\r\n                                                <p class=font><b>NOMBRE COMERCIAL: </b>{{nombre_comercial}}</p>\r\n                                            {% endif %}\r\n                                        {% else %}\r\n                                            <p class=font><b>NOMBRE O RAZON SOCIAL: </b>{{razon_social}}</p>\r\n                                        {% endif %}\r\n                                        \r\n                                        <!--<p class=font><b>NOMBRE O RAZON SOCIAL: </b>{{doc.customer}}</p>-->\r\n                                        <!--<p class=font><b>NOMBRE COMERCIAL: </b>{{doc.customer}}</p>-->\r\n                                        <p cla